{
  "presets": ["next/babel"],
  "plugins": [
    [
      "lodash",
      {
        "id": ["lodash", "recompose"]
      }
    ],
    [
      "transform-assets",
      {
        "extensions": ["txt", "svg", "png"],
        "regExp": ".*/static/(.+)",
        "name": "/static/[1]?[sha512:hash:base64:7]"
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "layouts": "./layouts",
          "utils": "./utils"
        }
      }
    ]
  ]
}
